{
    "version": 2,
    "builds": [
        { "src": "index.html", "use": "@vercel/static" },
        { "src": "server.js", "use": "@vercel/node" }
    ],
    "routes": [
        { "src": "/manifest.json", "dest": "/server.js" },
        { "src": "/catalog/(.*)", "dest": "/server.js" },
        { "src": "/meta/(.*)", "dest": "/server.js" },
        { "src": "/stream/(.*)", "dest": "/server.js" },
        { "src": "/", "dest": "/index.html" }
    ]
}